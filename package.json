{
  "name": "teamaster-io",
  "version": "1.0.0",
  "description": "multi-page app that gathers customer feedback with email surveys",
  "main": "/client/src/index.js",
  "engines": {
    "node": ">= 12.6.0",
    "npm": ">= 6.10.0"
  },
  "proxy": {
    "/api/*": {
      "target": "http://localhost:5000"
    }
  },
  "scripts": {
    "start:dev": "nodemon server/index.js",
    "build:dev": "webpack -d --watch --mode development --env.ENVIRONMENT=development",
    "webhook": "ssh -R teamaster-io:80:localhost:5000 serveo.net",
    "dev": "concurrently \"npm run start:dev\" \"npm run build:dev\" \"npm run webhook\"",
    "start": "node server/index.js",
    "heroku-postbuild": "webpack -p --env.ENVIRONMENT=production"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cookie-session": "^1.4.0",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "materialize-css": "^1.0.0",
    "mongoose": "^5.9.27",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "path-parser": "^6.1.0",
    "querystringify": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-stripe-checkout": "^2.6.3",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-thunk": "^2.3.0",
    "sendgrid": "^5.2.3",
    "stripe": "^8.81.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.0",
    "dotenv": "^8.2.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
